{
    "name": "{{ cookiecutter.project_name }} frontend",
    "version": "{{ cookiecutter.version }}",
    "description": "The project frontend stack.",
    "author": "David THENON <sveetch@gmail.com>",
    "private": true,
    "scripts": {
        "css": "sass --load-path=node_modules ./scss:../sandbox/static-sources/css/",
        "css-prod": "sass --style=compressed --load-path=node_modules ./scss:../sandbox/static-sources/css/",
        "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run-script css\"",
        "js": "webpack --mode development --progress",
        "js-prod": "webpack --mode production --progress",
        "watch-js": "nodemon --watch js/ --ext js --exec \"npm run-script js\""
    },
    "dependencies": {
        "@babel/cli": "^7.22.15",
        "@babel/core": "^7.22.17",
        "@babel/preset-env": "^7.22.15",
        "@popperjs/core": "^2.11.8",
        "autoprefixer": "^10.4.15",
        "babel-loader": "^9.1.3",
        "bootstrap": "5.3.2",
        "bootstrap-icons": "^1.11.1",
        "jquery": "^3.7.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "npm-run-all2": "^6.0.6",
        "sass": "^1.66.1",
        "stylelint": "^15.10.3",
        "stylelint-config-standard-scss": "^11.0.0",
        "webpack": "^5.89.0",
        "webpack-bundle-tracker": "^1.4.0",
        "webpack-cli": "^4.7.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": [
        ">= 0.5%",
        "last 2 major versions",
        "not dead",
        "Chrome >= 60",
        "Firefox >= 60",
        "Firefox ESR",
        "iOS >= 12",
        "Safari >= 12",
        "not Explorer <= 11"
    ]
}
